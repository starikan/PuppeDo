    "jest": "29.7.0",
    "playwright": "1.46.1",
    "prettier": "2.8.5",
    "prettier-eslint": "15.0.1",
    "puppeteer": "19.7.5",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4"
  }
