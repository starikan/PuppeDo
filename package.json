    "eslint-plugin-security": "3.0.1",
    "jest": "29.7.0",
    "playwright": "1.51.0",
    "prettier": "3.5.3",
    "prettier-eslint": "16.3.0",
    "puppeteer": "24.4.0",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "webpack": "5.98.0",
    "webpack-cli": "6.0.1"
  }
